{
  "name": "meme-archive-frontend",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.ts,.tsx",
    "lint:fix": "eslint . --ext .vue,.ts,.tsx --fix --fix-type problem,suggestion",
    "format:check": "prettier --config .prettierrc --check .",
    "format": "prettier --config .prettierrc --write .",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/node": "^22.0.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "eslint": "^9.0.0",
    "eslint-plugin-vue": "^9.26.0",
    "prettier": "^3.3.0",
    "typescript": "~5.4.0",
    "typescript-eslint": "^8.0.0",
    "vite": "^5.2.0",
    "vue-tsc": "^2.0.6"
  }
}
